###  https://vsr.informatik.tu-chemnitz.de/projects/2019/growth/wotdl#HeatingOn
:HeatingOn rdf:type owl:NamedIndividual ,
                    :HttpRequest ;
           :httpBody """required: true
content:
  application/x-www-form-urlencoded:
    schema:
      type: object
      properties:
        power:
          type: number
      required:
        - power"""^^xsd:string ;
           :httpMethod "POST"^^xsd:string ;
           :name "heating_on"^^xsd:string ;
           :url "http://10.0.1.113/heating/on"^^xsd:string .


###  https://vsr.informatik.tu-chemnitz.de/projects/2019/growth/wotdl#HeatingOnEffect
:HeatingOnEffect rdf:type owl:NamedIndividual ,
                          :FixedValueEffect ;
                 :affects :HeatingPowerState ;
                 :value "on"^^xsd:string .


